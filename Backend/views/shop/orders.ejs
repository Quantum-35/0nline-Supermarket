<%- include('../includes/head.ejs') %>
    </head>

    <body>
        <%- include('../includes/navigation.ejs') %>
        <main>
            <% if(orders.length > 0) {%>
                <ul>
                    <% orders.map(order => {%>
                        <li>
                            <h1><%= order.user._id%></h1>
                            <ul>
                                <% order.products.map(orderList => {%>
                                    <li><%= orderList.product.title%>(<%= orderList.quantity%>)</li>
                                <%})%>
                            </ul>
                        </li>
                    <% });%>
                    </ul>
            <%} else {%>
            <h1>Nothing there!</h1>
            <% }%>
        </main>
        <%- include('../includes/end.ejs') %>